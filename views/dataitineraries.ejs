                    <!-- your content here -->
                 
                    <form action="/dataitineraries/insert" method="POST" enctype="multipart/form-data">
                    <input type="text" class="form-control col-md-4" name="textheading" placeholder="Enter Heading of Tour"><br>
                            <input type="text" class="form-control col-md-4" name="nightdate" placeholder="Enter Tour Night Date"><br>
                            <input type="text" class="form-control col-md-4" name="link" placeholder="Enter Button Link"><br>
                            <input type="text" class="form-control col-md-4" name="touroverview" placeholder="Enter Tour OverView Heading">
                            <textarea cols="12" rows="12" class="form-control" name="overviewtext" placeholder="Enter OverView Text"></textarea><br>
                            <h4 style="color:white;">Insert Itineraries Side Image</h4>
                            <input type="file" class="form-control col-md-4" name="image"><br>
                            <input type="text" class="form-control col-md-4" name="fooddata" placeholder="Enter Food Data"><br>
                            <input type="text" class="form-control col-md-4" name="hoteldata" placeholder="Enter Hotel Data"><br>
                            <input type="text" class="form-control col-md-4" name="transportdata" placeholder="Enter Transport Data"><br>
                            <input type="text" class="form-control col-md-4" name="sightdata" placeholder="Enter Sightseeing Data"><br>

                            <input type="submit" value="SUBMIT" class="btn btn-danger"></form>
