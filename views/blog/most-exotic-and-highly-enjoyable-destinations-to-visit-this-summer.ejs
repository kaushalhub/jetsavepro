    function fillDropDown(id, data, label, selectedid = 0) {
        $(`#${id}`).empty();
        $(`#${id}`).append(
            $("<option>")
                .val("null")
                .text(label)
        );
        $.each(data, (i, item) => {
            if (item.id == selectedid) {
                $(`#${id}`).append(
                    $("<option selected>")
                        .val(item.id)
                        .text(item.blogareaname)
                );
            } else {
                $(`#${id}`).append(
                    $("<option>")
                        .val(item.id)
                        .text(item.blogareaname)
                );
            }
        });
    };

    function a(id, data, label, selectedid = 0) {
        $(`#${id}`).empty();
